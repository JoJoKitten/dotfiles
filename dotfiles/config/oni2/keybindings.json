[
  { "key": "<C-N>", "command": "workbench.action.quickOpen", "when": "editorTextFocus" },
  { "key": "<C-P>", "command": "workbench.action.showCommands", "when": "editorTextFocus" },
  { "key": "<C-V>", "command": "editor.action.clipboardPasteAction", "when": "editorTextFocus" },
  
  
  { "key": "<C-P>", "command": "list.focusUp", "when": "menuFocus || textInputFocus" },
  { "key": "<C-B>", "command": "list.focusDown", "when": "menuFocus || textInputFocus" },
  { "key": "<C-W>", "command": "explorer.togglee", "when": "editorTextFocus" },
  { "key": "<C-F>", "command": "workbench.action.quickOpenNavigateNextInEditorPicker", "when": "editorTextFocus" },
  { "key": "<C-S-F>", "command": "workbench.action.quickOpenNavigatePreviousInEditorPicker", "when": "editorTextFocus" }
]
