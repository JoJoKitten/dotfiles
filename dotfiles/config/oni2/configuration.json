{
  "ui.zoom": 1.0,
  "editor.detectIndentation": true,
  "editor.cursorSurroundingLines": 5,
  "editor.fontFamily": "JetBrainsMono-Regular.ttf",
  "editor.fontSize": 14,
  "editor.fontWeight": "normal",
  "editor.fontSmoothing": "subpixel-antialiased",
  "editor.lineNumbers": "relative",
  "editor.largeFileOptimizations": true,
  "editor.highlightActiveIndentGuide": true,
  "editor.indentSize": 4,
  "editor.insertSpaces": true,
  "editor.matchBrackets": true,
  "editor.minimap.enabled": true,
  "editor.minimap.maxColumn": 120,
  "editor.minimap.showSlider": true,
  "editor.renderIndentGuides": true,
  "editor.renderWhitespace": "selection",
  "editor.rulers": [ 80, 120 ],
  "editor.tabSize": 4,
  "editor.zenMode.hideTabs": true,
  "editor.zenMode.singleFile": true,
  "files.exclude": [ "_esy", "node_modules", ".git" ],
  "workbench.colorTheme": "Search Lights",
  "workbench.activityBar.visible": true,
  "window.menuBarVisibility": " hidden",
  "workbench.editor.showTabs": true,
  "workbench.iconTheme": "vs-seti",
  "workbench.sideBar.location": "left",
  "workbench.sideBar.visible": true,
  "workbench.statusBar.visible": true,
  "workbench.tree.indent": 2,
  "vim.leader": "<space>",
  "vim.timeout": 500,
  "vim.useSystemClipboard": [ "yank" ],
  "experimental.editor.cursorSmoothCaretAnimation": false,
  "experimental.editor.smoothScroll": false
}
